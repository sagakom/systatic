{
    "name": "damcclean/systatic",
    "description": "Systatic is a Powerful Static Site Generator for PHP Developers.",
    "authors": [
        {
            "name": "Duncan McClean",
            "email": "duncan@mcclean.co.uk"
        }
    ],
    "autoload": {
        "psr-4": {
            "Damcclean\\Systatic\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "require": {
        "php": "^7.2",
        "algolia/algoliasearch-client-php": "^2.2",
        "brick/varexporter": "^0.2.1",
        "erusev/parsedown-extra": "^0.7.1",
        "illuminate/config": "^5.8",
        "illuminate/console": "^5.8",
        "illuminate/container": "^5.8",
        "illuminate/events": "^5.8",
        "illuminate/filesystem": "^5.8",
        "jenssegers/blade": "^1.2",
        "nesbot/carbon": "^2.24",
        "nunomaduro/collision": "^3.0",
        "symfony/dotenv": "^4.3",
        "symfony/yaml": "^4.3",
        "tightenco/collect": "^6.0",
        "vlucas/phpdotenv": "^3.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^8",
        "symfony/var-dumper": "^4.3"
    },
    "config": {
        "sort-packages": true
    }
}